{
	"name": "wikidata-entity-store",
	"version": "3.0.0",
	"description": "Handles loading and caching of Wikidata Entities for you",
	"license": "MIT",
	"repository": "EdJoPaTo/wikidata-entity-store",
	"author": {
		"name": "EdJoPaTo",
		"email": "edjopato@gmail.com",
		"url": "https://edjopato.de"
	},
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"build": "del dist && tsc",
		"prepack": "npm run build",
		"test": "tsc --sourceMap && xo && nyc ava"
	},
	"main": "dist/source",
	"types": "dist/source",
	"files": [
		"dist/source",
		"!*.test.*"
	],
	"keywords": [
		"wikidata"
	],
	"dependencies": {
		"@edjopato/datastore": "^0.2.1",
		"js-yaml": "^3.13.1",
		"tableize-object": "^0.1.0",
		"wikibase-sdk": "^7.4.1",
		"wikidata-sdk-got": "^2.0.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^0.7.0",
		"@types/js-yaml": "^3.12.1",
		"@types/node": "^13.13.5",
		"ava": "^3.8.1",
		"del-cli": "^3.0.0",
		"nyc": "^15.0.1",
		"typescript": "^3.3.3",
		"xo": "^0.30.0"
	},
	"nyc": {
		"extension": [
			".ts"
		],
		"include": [
			"dist/source",
			"source"
		],
		"exclude": [
			"**/*.test.ts"
		],
		"all": true,
		"reporter": [
			"lcov",
			"text"
		]
	},
	"xo": {
		"rules": {
			"ava/no-ignored-test-files": "off"
		},
		"overrides": [
			{
				"files": [
					"test/**/*"
				],
				"rules": {
					"@typescript-eslint/prefer-readonly-parameter-types": "off"
				}
			}
		]
	}
}
